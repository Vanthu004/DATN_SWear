{
  "expo": {
    "name": "DATN_SWear",
    "slug": "DATN_SWear",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "datnswear",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff"
        }
      ]
    ]
  }
}
