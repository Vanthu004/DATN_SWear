{
  "expo": {
    "name": "DATN_SWear",
    "slug": "DATN_SWear",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "datnswear",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.vanthu.DATN_SWear"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-web-browser",
      "react-native-edge-to-edge"
    ],
    "extra": {
      "eas": {
        "projectId": "a2cea276-0297-4d80-b5bd-d4f21792a83a"
      }
    },
    "owner": "vanthu"
  }
}
